{
  "name": "expression-language",
  "version": "2.5.0",
  "description": "Javascript implementation of symfony/expression-language",
  "main": "lib/index.js",
  "browser": "dist/expression-language.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameskfry/expression-language.git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "build:browser": "rollup -c",
    "build:all": "npm run build && npm run build:browser",
    "prepublishOnly": "npm run build:all",
    "test": "jest"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "reporters": ["default", "<rootDir>/jest-reporter.cjs"]
  },
  "keywords": [
    "expression",
    "language"
  ],
  "author": "James K Fry",
  "license": "MIT",
  "dependencies": {
    "locutus": "^2.0.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "jest": "^30.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
